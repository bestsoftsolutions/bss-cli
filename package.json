{
  "name": "bss-cli",
  "version": "1.0.1",
  "description": "A TypeScript CLI application",
  "type": "module",
  "main": "dist/index.cjs",
  "bin": {
    "bss-cli": "dist/index.cjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.cjs",
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --format cjs",
    "publish:public": "node scripts/publish.cjs"
  },
  "keywords": [
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.19.30",
    "@types/oracledb": "^6.10.0",
    "fs-extra": "^11.3.3",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "eta": "^4.5.0",
    "inquirer": "^13.1.0",
    "oracledb": "^6.10.0",
    "tsup": "^8.5.1"
  }
}
